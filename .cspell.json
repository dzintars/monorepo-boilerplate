{
    "version": "0.1",
    "language": "en",
    "ignorePaths": [
        "**/coverage/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/fixtures/**",
        "**/**/CHANGELOG.md",
        "**/**/CONTRIBUTORS.md",
        "**/**/ROADMAP.md",
        "**/*.{json,snap}"
    ],
    "dictionaries": [
        "typescript",
        "softwareTerms",
        "node",
        "en_US",
        "npm",
        "misc",
        "filetypes"
    ],
    "ignoreRegExpList": [
        "/```[\\w\\W]*?```/",
        "/~~~[\\w\\W]*?~~~/",
        "/``[\\w\\W]*?``/",
        "/`[^`]*`/",
        "/\\.\\/docs\\/rules\\/[^.]*.md/",
        "/@typescript-eslint\\/[a-z-]+/",
        "/\\.all-contributorsrc/",
        "/TS[^\\s]+/"
    ],
    "words": [
        "bigint",
        "commitlint",
        "dbaeumer",
        "Dzintars",
        "esbenp",
        "gitignore",
        "gmail",
        "huskyrc",
        "Hygen",
        "Klavins",
        "lintstaged",
        "npmignore",
        "nvmrc",
        "prettierignore",
        "prettierrc",
        "ROADMAP",
        "tsbuildinfo"
    ],
    "overrides": [
        {
            "filename": "**/*.{ts,js}",
            "ignoreRegExpList": [
                "/@[a-z]+/",
                "/#(end)?region/"
            ],
            "includeRegExpList": [
                "/\\/\\*[\\s\\S]*?\\*\\/|([^\\\\:]|^)\\/\\/.*$/",
                "/(\\/\\/[^\\n\\r]*[\\n\\r]+)/"
            ]
        }
    ]
}